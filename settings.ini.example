[django]
debug           = True
secret          = xxxxxxxx
allowed_hosts   = *
language_code   = en-us
time_zone       = America/Los_Angeles
static_root     = /var/www/html/static
sqlite_db       = db.sqlite3

[logging]
app_level       = DEBUG
app_handler     = console
django_level    = INFO
django_handler  = console
log_file        = django.log

[database]
